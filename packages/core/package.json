{
  "name": "@substreams/core",
  "version": "0.1.19",
  "license": "MIT",
  "scripts": {
    "codegen": "rm -rf src/proto && buf generate buf.build/streamingfast/substreams",
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./proto": {
      "types": "./dist/proto.d.ts",
      "default": "./dist/proto.js"
    },
    "./proto/*": {
      "types": "./dist/proto/*.d.ts",
      "default": "./dist/proto/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "proto": [
        "./dist/proto.d.ts"
      ],
      "proto/*": [
        "./dist/proto/*.d.ts"
      ]
    }
  },
  "files": [
    "dist/**",
    "!dist/**/.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/substreams-js/substreams-js.git",
    "directory": "packages/core"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.3.1",
    "@connectrpc/connect": "^0.13.2"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.26.1",
    "@types/node": "^20.7.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.5"
  }
}
